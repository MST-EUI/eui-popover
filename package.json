{
  "name": "@mistong/eui-popover",
  "version": "0.1.0",
  "description": "a popover react component",
  "main": "dist/index.js",
  "scripts": {
    "start": "eui server",
    "dev": "eui server",
    "build": "eui build",
    "test": "eui test",
    "commit": "git-cz",
    "coverage": "eui test --coverage",
    "lint": "eslint --fix src/*",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "contributors": [
    "mistong eui"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MST-EUI/EUI-component-tpl.git"
  },
  "bugs": {
    "url": "https://github.com/MST-EUI/EUI-component-tpl/issues"
  },
  "homepage": "https://github.com/MST-EUI/EUI-component-tpl",
  "keywords": [
    "react",
    "react-component",
    "eui"
  ],
  "files": [
    "dist",
    "src",
    "demo"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@mistong/eui": "latest",
    "@mistong/eui-cli": "latest",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^6.2.0",
    "conventional-changelog-cli": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-14": "^1.1.0",
    "es3ify-loader": "^0.2.0",
    "es5-shim": "^4.5.10",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "react": "^0.14.9",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.9"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "rc-tooltip": "^3.7.2",
    "prop-types": "^15.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add ."
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  }
}

{
  "name": "@mistong/eui-popover",
  "version": "0.1.0",
  "description": "a popover react component",
  "main": "dist/index.js",
  "scripts": {
    "start": "eui server",
    "dev": "eui server",
    "build": "eui build",
    "test": "eui test",
    "coverage": "eui test --coverage",
    "lint": "eui lint",
    "lint:fix": "eui lint --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "contributors": [
    "mistong eui"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MST-EUI/EUI-component-tpl.git"
  },
  "bugs": {
    "url": "https://github.com/MST-EUI/EUI-component-tpl/issues"
  },
  "homepage": "https://github.com/MST-EUI/EUI-component-tpl",
  "keywords": [
    "react",
    "react-component",
    "eui"
  ],
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@mistong/eui-button": "^0.1.3",
    "@mistong/eui-cli": "^0.1.17",
    "@mistong/eui-css": "^0.1.2",
    "conventional-changelog-cli": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-14": "^1.0.5",
    "es5-shim": "^4.5.10",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "react": "^0.14.9",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.9"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "rc-tooltip": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add ."
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  }
}
